{
    "configurations": [
        {
            "name": "main",
            "compilerPath": "/usr/bin/gcc",
            "defines": [
                "DEBUG"
            ],
            "compilerArgs": [
                "-std=gnu++20"
            ],
            "includePath": [
                "./include",
                "./external_libs/libcurlcppxx/include/base",
                "./external_libs/libcurlcppxx/include/http",
                "./external_libs/libcurlcppxx/include/ext"
            ],
            "cppStandard": "${default}",
            "intelliSenseMode":"linux-clang-x64",
            "browse": {
                "databaseFilename": "${workspaceFolder}/.vscode/browse.out",
                "limitSymbolsToIncludedHeaders": false,
                "path":["${workspaceFolder}"]
            }
		}
	],
    "version": 4
}